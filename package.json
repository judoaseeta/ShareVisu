{
  "name": "reactssr",
  "version": "1.0.0",
  "description": "React SSR Boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev && node ./dist/server.js",
    "start:dev": "webpack-dev-server --env=dev --profile --colors",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -rf dist/ && NODE_ENV=production npm run build:client && NODE_ENV=production npm run build:server",
    "build:dev": "rm -rf dist/ && NODE_ENV=development npm run build:client && NODE_ENV=development npm run build:server",
    "build:server": "webpack --env=server --progress --profile --colors",
    "build:client": "webpack --env=client --progress --profile --colors",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/judoaseeta/reactSSR.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/judoaseeta/reactSSR/issues"
  },
  "homepage": "https://github.com/judoaseeta/reactSSR#readme",
  "dependencies": {
