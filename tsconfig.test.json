{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "jsx": "react",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitThis": false,
      "moduleResolution": "node",
      "baseUrl": "./",
      "esModuleInterop": true,
    },
    "exclude": [
      "**/*.test.*",
      "**/*.spec.*",
      "node_modules",
      "dist",
      "public"
    ]
  }
  